function TrainRoadwork(CD)
    spawn(function()
        local plr = game.Players.LocalPlayer
        local character = plr.character
        local Humanoid = character:FindFirstChild("Humanoid")
        local HRP = character:FindFirstChild("HumanoidRootPart")
        local Debounce = false
        
        local RoadWorkFolder = game:GetService("Workspace").Roadwork

        while getgenv().SemiAutoRoadWork == true do wait()
             local RoadworkItem = plr.Backpack:FindFirstChild("Roadwork") or character:FindFirstChild("Roadwork")
    
                if Debounce == false and RoadworkItem then
                    Debounce = true
                   RoadworkItem.Parent = game.Players.LocalPlayer.Character
                   RoadworkItem.Equipped:Connect(function()
                         RoadworkItem:Activate()
                     end)
                    
                HRP.CFrame = RoadWorkFolder["1"].CFrame
                game.Players.LocalPlayer.Character.Humanoid.Jump = true
                wait(CD)
                HRP.CFrame = RoadWorkFolder["2"].CFrame
                game.Players.LocalPlayer.Character.Humanoid.Jump = true
                wait(CD)
                HRP.CFrame = RoadWorkFolder["3"].CFrame
                game.Players.LocalPlayer.Character.Humanoid.Jump = true
                wait(CD)
                HRP.CFrame = RoadWorkFolder["4"].CFrame
                game.Players.LocalPlayer.Character.Humanoid.Jump = true
                wait(CD)
                HRP.CFrame = RoadWorkFolder["5"].CFrame
                game.Players.LocalPlayer.Character.Humanoid.Jump = true
                wait(CD)
                HRP.CFrame = RoadWorkFolder["6"].CFrame
                game.Players.LocalPlayer.Character.Humanoid.Jump = true
                wait(CD)
                HRP.CFrame = RoadWorkFolder["7"].CFrame
                game.Players.LocalPlayer.Character.Humanoid.Jump = true
                wait(CD)
                HRP.CFrame = RoadWorkFolder["8"].CFrame
                game.Players.LocalPlayer.Character.Humanoid.Jump = true
                wait(CD)
                HRP.CFrame = RoadWorkFolder["7"].CFrame * CFrame.new(27.3, 0, -79)
                Debounce = false
                else
                    HRP.CFrame = RoadWorkFolder["7"].CFrame * CFrame.new(27.3, 0, -79)
                end
             end
    end)
end

function ClanReroll(CD)
    spawn(function()

        while getgenv().AutoClanReroll == true do task.wait(tonumber(CD))
            
            local Name = game:GetService("Players").AlkalydLotus.PlayerGui.MainGui.StatusFrame["Name/Clan"]
            local FoundName = Name.Text
            
            local ClanWanted = {"Ha√±ma","Fujin", "Kagutsuchi", "Raijin", "Ryu", "Sung"}
            
            FoundName:gsub('(.*) (.*)', function (First, Clan)
                Find = table.find(ClanWanted, Clan)
            end)
            
            if Find then
                print("Clan Found")
                getgenv().AutoClanReroll = false
            break
            
            elseif not Find then
                
                local args = {[1] = "BP"}
                game:GetService("ReplicatedStorage").Reroll:FireServer(unpack(args))
            end
        end
    end)
end

function AutoEat(v11, v22, CDD)
spawn(function()
    local plr = game.Players.LocalPlayer
    local character = plr.character
    local Humanoid = character:FindFirstChild("Humanoid")
    local HRP = character:FindFirstChild("HumanoidRootPart")

    local Check = plr.Backpack:GetChildren() or character:GetChildren()

    local HungerText = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.StatusFrame.Hunger.stat
    local Hungervalue = HungerText.Text 

    Hungervalue:gsub('(.*)/(.*)', function (Current, Max)
        NumCurrent = tonumber(Current)
    end)
    
    local CD = tonumber(CDD)
    
    while getgenv().AutoEat == true do wait(CD)
         local v1 = tonumber(v11)
         local v2 = tostring(v22)
         
         if NumCurrent <= v1 then
            for i, v in pairs(Check) do
                if v.Name == v2 then 
                    Humanoid:EquipTool(v)
                    v:Activate()
                    wait(3)
                    Humanoid:UnequipTool()
                end
            end
        end
    end
end)
end

