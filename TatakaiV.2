function TrainStrength()
    spawn(function()
        
        local plr = game.Players.LocalPlayer
            local character = plr.character
            local Humanoid = character:FindFirstChild("Humanoid")
            local HRP = character:FindFirstChild("HumanoidRootPart")
            local Debounce = false
            local Glovess = false
            
            local PB = game.Workspace["Punching Bags"]:GetDescendants()
            
            local Bags = {}
            local gg
            local pg
            
            for i, v in pairs(PB) do
               if v.Name == "HumanoidRootPart" then
                   gg = table.find(Bags, v)
                   
                   if not gg then
                   table.insert(Bags, v)
                   end
                   
                   pg = Bags[1].CFrame
                end
            end
                   TT = table.unpack(Bags)
                   
                   local Num = TT
        
        while getgenv().SemiAutoStrength == true do wait()
             local Combats = plr.Backpack:FindFirstChild("Combat") or character:FindFirstChild("Combat")
                local GloveItem = plr.Backpack:FindFirstChild("Gloves") or character:FindFirstChild("Gloves")
                if Debounce == false and GloveItem then
                    HRP.CFrame = Num.CFrame
                   HRP.CFrame = HRP.CFrame * CFrame.new(0, 0, 1)
                   
                   if Glovess == false then
                       Combats.Parent = game.Players.LocalPlayer.Character
                       
                     GloveItem.Parent = game.Players.LocalPlayer.Character
                     GloveItem.Equipped:Connect(function()
                         GloveItem:Activate()
                         Glovess = true
                     end)
                
                    Combats.Equipped:Connect(function()
                        while Glovess == true do wait()
                        Combats:Activate()
                        end
                        end)
                    end
                   else
                       Glovess = false
                      Humanoid:UnequipTools()
                       
                        local first = game.Workspace.Items:GetDescendants()
                        for i, v in pairs(first) do
                             if v.Name == "Gloves $75" then
                                local Part = v:FindFirstChild("Head")
                                 HRP.CFrame = Part.CFrame * CFrame.new(0, 1, 0)
                             end
                        end
                end
            end
--
    end)
end

function TrainRoadwork(CDD)
    spawn(function()
        local plr = game.Players.LocalPlayer
        local character = plr.character
        local Humanoid = character:FindFirstChild("Humanoid")
        local HRP = character:FindFirstChild("HumanoidRootPart")
        local CD = tonumber(CDD)
        local Debounce = false
        
        local RoadWorkFolder = game:GetService("Workspace").Roadwork

        while getgenv().SemiAutoRoadWork == true do wait()
             local RoadworkItem = plr.Backpack:FindFirstChild("Roadwork") or character:FindFirstChild("Roadwork")
    
                if Debounce == false and RoadworkItem then
                    Debounce = true
                   RoadworkItem.Parent = game.Players.LocalPlayer.Character
                   RoadworkItem.Equipped:Connect(function()
